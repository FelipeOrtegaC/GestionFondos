<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:id="@+id/linearLayout3"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="1dp"
        android:layout_marginTop="1dp"
        android:layout_marginEnd="1dp"
        android:layout_marginBottom="1dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:background="@drawable/degradado">

        <View
            android:id="@+id/view3"
            android:layout_width="match_parent"
            android:layout_marginTop="20dp"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="20dp"
            android:layout_height="156dp"
            android:background="@drawable/whitelogo"
            />

        <TextView
            android:id="@+id/textView"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="20dp"
            android:layout_marginTop="20dp"
            android:layout_marginBottom="20dp"
            android:textSize="16dp"
            android:textColor="@color/golden"
            android:fontFamily="@font/neuemachinaultrabold"
            android:text="@string/manual" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="20dp"
            android:textSize="13dp"
            android:textColor="@color/golden"
            android:fontFamily="@font/neuemachinaregular"
            android:text="@string/manualextend" />

        <View
            android:id="@+id/view4"
            android:layout_width="wrap_content"
            android:layout_height="28dp"
            android:layout_marginLeft="40dp"
            android:layout_marginTop="10dp"
            android:layout_marginRight="40dp"
            android:background="@drawable/examplebar" />

        <TextView
            android:id="@+id/textView3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="20dp"
            android:layout_marginBottom="10dp"
            android:textSize="13dp"
            android:textColor="@color/golden"
            android:fontFamily="@font/neuemachinaregular"
            android:text="@string/manual1" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="20dp"
            android:layout_marginBottom="10dp"
            android:textSize="13dp"
            android:textColor="@color/white"
            android:fontFamily="@font/neuemachinaregular"
            android:text="@string/manual2" />


        <TextView
            android:id="@+id/textView6"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="20dp"
            android:layout_marginBottom="10dp"
            android:textSize="13dp"
            android:textColor="@color/white"
            android:fontFamily="@font/neuemachinaregular"
            android:text="@string/manual3" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20dp"
            android:layout_marginBottom="10dp"
            android:layout_marginRight="20dp"
            android:textSize="13dp"
            android:textColor="@color/white"
            android:fontFamily="@font/neuemachinaregular"
            android:text="@string/manual4" />

        <TextView
            android:id="@+id/textView8"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="20dp"
            android:layout_marginBottom="10dp"
            android:textSize="13dp"
            android:textColor="@color/white"
            android:fontFamily="@font/neuemachinaregular"
            android:text="@string/manual5" />

        <TextView
            android:id="@+id/textView9"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="20dp"
            android:layout_marginBottom="10dp"
            android:textSize="13dp"
            android:textAlignment="center"
            android:textColor="@color/golden"
            android:fontFamily="@font/neuemachinaultrabold"
            android:text="@string/supportcont" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="75dp"
            android:orientation="horizontal"
            android:layout_marginBottom="5dp">

            <ImageButton
                android:id="@+id/imageButton"
                android:layout_width="69dp"
                android:layout_height="69dp"
                android:layout_weight="1"
                android:layout_marginBottom="5dp"
                android:layout_marginLeft="5dp"
                android:background="@drawable/mailbtn" />

            <ImageButton
                android:id="@+id/imageButton2"
                android:layout_width="69dp"
                android:layout_height="65dp"
                android:layout_marginTop="3dp"
                android:layout_weight="1"
                android:background="@drawable/webpagebtn" />

            <ImageButton
                android:id="@+id/imageButton3"
                android:layout_width="69dp"
                android:layout_height="69dp"
                android:layout_weight="1"
                android:background="@drawable/fbbtn" />

            <ImageButton
                android:id="@+id/imageButton4"
                android:layout_width="69dp"
                android:layout_height="69dp"
                android:layout_marginLeft="5dp"
                android:layout_weight="1"
                android:background="@drawable/instabtn" />

            <ImageButton
                android:id="@+id/imageButton5"
                android:layout_width="70dp"
                android:layout_height="69dp"
                android:layout_marginLeft="5dp"
                android:layout_weight="1"
                android:background="@drawable/phonebtn" />

            <ImageButton
                android:id="@+id/imageButton6"
                android:layout_width="69dp"
                android:layout_height="69dp"
                android:layout_weight="1"
                android:layout_marginRight="5dp"
                android:background="@drawable/linkedbtn" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="70dp"
            android:layout_marginTop="5dp"
            android:layout_gravity="end"
            android:background="@color/gray"
            android:orientation="horizontal">

            <ImageButton
                android:id="@+id/imageButton7"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                android:layout_marginBottom="8dp"
                android:layout_marginLeft="12dp"
                android:layout_marginRight="12dp"
                android:layout_weight="1"
                app:srcCompat="@drawable/metrics1" />

            <ImageButton
                android:id="@+id/imageButton8"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                android:layout_marginBottom="8dp"
                android:layout_marginLeft="12dp"
                android:layout_marginRight="12dp"
                android:layout_weight="1"
                app:srcCompat="@drawable/gestion1" />

            <ImageButton
                android:id="@+id/imageButton9"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                android:layout_marginBottom="8dp"
                android:layout_marginLeft="12dp"
                android:layout_marginRight="12dp"
                android:layout_weight="1"
                app:srcCompat="@drawable/home2" />

            <ImageButton
                android:id="@+id/imageButton10"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                android:layout_marginBottom="8dp"
                android:layout_marginLeft="12dp"
                android:layout_marginRight="12dp"
                android:layout_weight="1"
                app:srcCompat="@drawable/stock1" />

            <ImageButton
                android:id="@+id/imageButton11"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                android:layout_marginBottom="8dp"
                android:layout_marginLeft="12dp"
                android:layout_marginRight="12dp"
                android:layout_weight="1"
                app:srcCompat="@drawable/config1" />
        </LinearLayout>

    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>